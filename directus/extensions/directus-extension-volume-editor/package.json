{
  "name": "directus-extension-volume-editor",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "type": "module",
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "module",
        "name": "volume-editor",
        "source": "src/volume-editor/index.js"
      },
      {
        "type": "hook",
        "name": "configure-vueform",
        "source": "src/configure-vueform/index.js"
      }
    ],
    "host": "^10.3.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "10.3.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@vueform/vueform": "^1.7.1",
    "vee-validate": "^4.12.4",
    "vuedraggable": "^4.1.0"
  }
}
